<h1>
    Employees MongoDB
</h1>
<style>
    th, td, table{
        border: 2px solid pink;
    }
</style>

<button data-ref="" class="btn btn-primary">Add Employee MongoDB</button>

<br>
<table border="1">
    <tr>
    <th>EID</th>
    <th>Phone</th>
    <th>Email</th>
    </tr>
    <% MongoEmployees.forEach((employeeMongodb) => { %>
    <tr>
    <td><%= employeeMongodb._id %></td>
    <td><%= employeeMongodb.phone %></td>
    <td><%= employeeMongodb.email %></td>
    </tr>
    <% }) %>
</table>
<br>
<a href="/">Home</a>